[application]

config/name="Superb Demo Survivors"
run/main_scene="res://Scenes/Level_Stage1.tscn"
config/icon="res://icon.png"

[autoload]

MapManager="*res://Scripts/Managers/MapManager.gd"
PlayerManager="*res://Scripts/Managers/PlayerManager.gd"
TimeManager="*res://Scripts/Managers/TimeManager.gd"
MenuManager="*res://Scripts/Managers/MenuManager.gd"
EquipmentManager="*res://Scripts/Managers/EquipmentManager.gd"
EnemyManager="*res://Scripts/Managers/EnemyManager.gd"

[display]

window/size/width=768
window/size/height=432
window/size/resizable=false
window/size/test_width=1536
window/size/test_height=864
window/vsync/use_vsync=false
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="walls"
2d_physics/layer_2="player"
2d_physics/layer_3="pickups"
2d_physics/layer_4="enemies"
2d_physics/layer_5="weapons"

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_environment="res://default_env.tres"
